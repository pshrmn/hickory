{
  "name": "hickory",
  "version": "0.2.0",
  "description": "Hickory powers navigation within a single page application",
  "main": "dist/hickory.common.js",
  "module": "dist/hickory.es.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node ./scripts/build",
    "prepublishOnly": "in-publish && npm run build",
    "test": "npm run test:unit && npm run test:integration -- --single-run",
    "test:integration": "karma start",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pshrmn/hickory.git"
  },
  "keywords": [
    "history",
    "navigation",
    "single page application"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pshrmn/hickory/issues"
  },
  "homepage": "https://github.com/pshrmn/hickory#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "gzip-size": "^3.0.0",
    "in-publish": "^2.0.0",
    "jasmine": "^2.6.0",
    "jest": "^20.0.4",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-plugin": "^0.2.4",
    "pretty-bytes": "^4.0.2",
    "qs": "^6.4.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/unit/**/*.spec.js"
    ]
  },
  "dependencies": {}
}
