{
  "name": "@hickory/browser",
  "version": "1.0.0-beta.5",
  "description": "Hickory's browser history",
  "main": "dist/hickory-browser.common.js",
  "module": "dist/hickory-browser.es.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist & rimraf types",
    "build": "node ./scripts/build",
    "prepublishOnly": "npm run build",
    "test": "node ./scripts/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/hickory/tree/master/packages/browser"
  },
  "keywords": [
    "Hickory",
    "history",
    "browser"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "dependencies": {
    "@hickory/dom-utils": "^1.0.0-beta.1",
    "@hickory/root": "^1.0.0-beta.6"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "jasmine": "^2.8.0",
    "jest": "^21.1.0",
    "karma": "^1.7.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^5.0.1",
    "qs": "^6.5.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "globals": {
      "ts-jest": {
        "module": "es6"
      }
    }
  }
}
