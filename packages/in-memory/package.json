{
  "name": "@hickory/in-memory",
  "version": "1.0.0-beta.3",
  "description": "Hickory's in-memory history",
  "main": "dist/hickory-in-memory.common.js",
  "module": "dist/hickory-in-memory.es.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist && rimraf types",
    "build": "tsc && node ./scripts/build",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/hickory/tree/master/packages/in-memory"
  },
  "keywords": [
    "Hickory",
    "history",
    "memory"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "dependencies": {
    "@hickory/root": "^1.0.0-beta.6"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "jest": "^21.1.0",
    "qs": "^6.5.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/tests/*.spec.ts"
    ],
    "globals": {
      "ts-jest": {
        "module": "es6"
      }
    }
  }
}
